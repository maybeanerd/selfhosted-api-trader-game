{
  "name": "selfhosted-api-trader-game",
  "version": "0.0.1",
  "description": "A WIP game.",
  "author": "Sebastian Di Luzio",
  "packageManager": "pnpm@8.9.0",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/T0TProduction/selfhosted-api-trader-game.git"
  },
  "bugs": {
    "url": "https://github.com/T0TProduction/selfhosted-api-trader-game/issues"
  },
  "homepage": "https://github.com/T0TProduction/selfhosted-api-trader-game#readme",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:all": "pnpm run test && pnpm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.7",
    "@nestjs/core": "10.2.7",
    "@nestjs/platform-fastify": "10.2.7",
    "@nestjs/schedule": "3.0.4",
    "@nestjs/sequelize": "10.0.0",
    "@nestjs/swagger": "7.1.13",
    "axios": "1.5.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.3.1",
    "pg": "8.11.3",
    "pg-hstore": "2.3.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sequelize": "6.33.0",
    "sequelize-typescript": "2.1.5"
  },
  "devDependencies": {
    "@fastify/static": "6.11.2",
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.7",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.19",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.4",
    "@types/sequelize": "4.28.16",
    "@types/supertest": "2.0.14",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint": "8.51.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  }
}
